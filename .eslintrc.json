{
  "root": true,
  "plugins": [
    "react",
    "flowtype"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "react-app",
    "plugin:flowtype/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-cond-assign": [1, "except-parens"],
    "no-console": 1,
    "no-debugger": 1,
    "array-callback-return": 1,
    "block-scoped-var": 2,
    "class-methods-use-this": 1,
    "complexity": 0,
    "curly": 2,
    "default-case": 2,
    "dot-location": [2, "property"],
    "eqeqeq": 2,
    "no-alert": 2,
    "no-caller": 1,
    "no-case-declarations": 2,
    "no-empty-function": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": [2, { "commentPattern": "break[\\s\\w]*omitted" }],
    "no-floating-decimal": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 1,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-loop-func": 1,
    "no-magic-numbers": [1, { "ignore": [-1, 0, 1] }],
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-return-await": 1,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": [2, { "allowShortCircuit": true, "allowTernary": true, "allowTaggedTemplates": true }],
    "no-useless-call": 2,
    "no-useless-concat": 1,
    "no-useless-return": 2,
    "no-void": 2,
    "no-with": 2,
    "prefer-promise-reject-errors": 1,
    "radix": [2, "as-needed"],
    "require-await": 2,
    "wrap-iife": [2, "any"],
    "yoda": 1,
    "no-undef-init": 2,
    "no-label-var": 2,
    "no-restricted-globals": 1,
    "no-shadow-restricted-names": 2,
    "array-bracket-spacing": 1,
    "block-spacing": [1, "never"],
    "brace-style": 1,
    "camelcase": 1,
    "comma-dangle": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "func-call-spacing": 1,
    "key-spacing": 1,
    "keyword-spacing": [1, { "overrides": {
      "if": { "after": false },
      "for": { "after": false },
      "while": { "after": false },
      "switch": { "after": false }
    }}],
    "max-len": [1, 120, { "ignoreUrls": true }, { "ignoreRegExpLiterals": true }],
    "newline-per-chained-call": [1, { "ignoreChainWithDepth": 3 }],
    "no-inline-comments": 1,
    "no-lonely-if": 1,
    "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 1,
    "object-curly-spacing": 0,
    "one-var": 0,
    "operator-assignment": 1,
    "quotes": [1, "single"],
    "semi": 2,
    "space-before-function-paren": [1, {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "ignore"
    }],
    "space-in-parens": 1,
    "wrap-regex": 2,
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": 1,
    "generator-star-spacing": 0,
    "no-duplicate-imports": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-const": 1,
    "prefer-destructuring": 0,
    "prefer-numeric-literals": 1,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "rest-spread-spacing": 1,
    "symbol-description": 1,
    "template-curly-spacing": 1,
    "jsx-quotes": 1,
    "react/no-children-prop": 2,
    "react/no-danger-with-children": 2,
    "react/no-deprecated": 2,
    "react/no-find-dom-node": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-render-return-value": 1,
    "react/no-string-refs": 2,
    "react/display-name": [ 1, {"ignoreTranspilerName": false }],
    "react/jsx-equals-spacing": 1,
    "react/jsx-boolean-value": 0,
    "react/jsx-closing-bracket-location": [1, {"selfClosing": "line-aligned", "nonEmpty": "line-aligned"}],
    "react/jsx-curly-spacing": 1,
    "react/jsx-indent-props": 0,
    "react/jsx-key": 2,
    "react/jsx-max-props-per-line": 0,
    "react/jsx-no-bind": [2, {
      "ignoreRefs": true,
      "allowArrowFunctions": true
    }],
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-literals": 0,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 1,
    "react/jsx-sort-props": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-wrap-multilines": 1,
    "react/jsx-no-target-blank": 2,
    "react/jsx-space-before-closing": [1, "never"],
    "react/jsx-tag-spacing": [1, {
      "closingSlash": "never",
      "beforeSelfClosing": "never",
      "afterOpening": "never"
    }],
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-direct-mutation-state": 1,
    "react/no-multi-comp": 0,
    "react/no-set-state": 0,
    "react/no-unknown-property": 1,
    "react/prefer-es6-class": 2,
    "react/prefer-stateless-function": 1,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 1,
    "import/extensions": 1,
    "react/self-closing-comp": 1
  },
  "globals": {}
}
